filteredGoods = this.goods.filter(good => regexp.test(good.product_name));\n    },\n\n    showBasket(){\n      this.isVisibleCart = !this.isVisibleCart;\n    },\n\n    addToBasket(item){\n      this.basketList.push(item);\n    }\n  },\n\n}\n</script>\n\n\n<style>\n.main_wrapper {\n  margin: 0 auto;\n  max-width: 1920px;\n  padding: 50px;\n}\n</style>\n"],"sourceRoot":""}]}