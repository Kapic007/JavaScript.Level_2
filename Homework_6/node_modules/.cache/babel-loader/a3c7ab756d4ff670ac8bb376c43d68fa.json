,EAAC,eAAkE;AAAlD,IAAA,IAAI,EAAC,QAA6C;AAAnC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,WAAA,IAAA,QAAA,CAAA,WAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAA8B,GAAhF,EAAiE,QAAjE,CAIO,EAHP,YAAA,CAES,QAFT,EAES;AAFD,IAAA,KAAK,EAAC,aAEL;AAFmB,IAAA,IAAI,EAAC,QAExB;AAFkC,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,UAAA,IAAA,QAAA,CAAA,UAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAEvC,GAFT,EAA8D,WAA9D,CAGO,CANX,C,EAOE,YAAA,CASO,MATP,EAAA,UAAA,EASO,CARL,YAAA,CAMM,KANN,EAAA,UAAA,EAMM,E,kBALJ,YAAA,CAIM,SAJN,EAIM,IAJN,EAIM,WAAA,CAJc,IAAA,CAAA,aAId,EAJ2B,UAArB,IAAqB,EAAjB;yBAAhB,YAAA,CAIM,KAJN,EAIM;AAJ8B,MAAA,GAAG,EAAE,IAAI,CAAC,UAIxC;AAJoD,MAAA,KAAK,EAAC;AAI1D,KAJN,E,CACE,YAAA,CAAuD,IAAvD,EAAA,UAAA,EAAuD,gBAAA,CAAxB,IAAI,CAAC,YAAmB,CAAvD,EAAgD;AAAA;AAAhD,K,EACA,YAAA,CAA8C,GAA9C,EAAA,UAAA,EAA8C,gBAAA,CAAhB,IAAI,CAAC,KAAW,CAA9C,EAAwC;AAAA;AAAxC,K,EACA,U,CAHF,C;GAIM,CAJN,E;;AAAA,GAKI,EANN,CAQK,E,gBADL,YAAA,CAA2D,KAA3D,EAAA,UAAA,EAA2D,IAA3D,EAA2D;AAAA;AAA3D,G,EAA2D,C,SAArB,IAAA,CAAA,a,CAAqB,C,CACtD,CATP,C","sourcesContent":["<template>\n  <header class=\"header\">\n    <input type=\"text\" class=\"goods-search\" v-model=\"searchLine\" @keyup=\"filterGoods\"/>\n      <button class=\"search-button\" type=\"button\" @click=\"filterGoods\">Искать</button>\n      <button class=\"cart-button\" type=\"button\" @click=\"showBasket\">\n        Корзина\n      </button>\n    </header>\n    <main class=\"main_wrapper\">\n      <div class=\"goods-list\">\n        <div v-for=\"item in filteredGoods\" :key=\"item.id_product\" class='goods-item'>\n          <h3 class=\"goods-item_title\">{{item.product_name}}</h3>\n          <p class=\"goods-item_price\">{{item.price}}</p>\n          <button class=\"button_add\">Добавить</button>\n        </div>\n      </div>\n      <div class=\"basket-container\" v-show=\"isVisibleCart\"></div>\n    </main>\n</template>\n\n<script>\n\nconst API_URL =\n  \"https://raw.githubusercontent.com/GeekBrainsTutorial/online-store-api/master/responses\";\n\nexport default {\n  name: 'App',\n  \n  data: () => ({\n    goods:[],\n    filteredGoods:[],\n    searchLine: '',\n    isVisibleCart: false,\n  }),\n\n  components: {},\n\n  mounted() {\n    this.makeGETRequest(`${API_URL}/catalogData.json`)\n  },\n\n  methods: {\n    makeGETRequest(url){\n      fetch(url).then((res) => res.json()).then((res)=>{\n        this.goods = res;\n        this.filteredGoods = res;\n      })\n    },\n\n    filterGoods(){\n      const regexp = new RegExp(this.searchLine, \"i\");\n      this.filteredGoods = this.goods.filter(good => regexp.test(good.product_name));\n\n    },\n    showBasket(){\n      this.isVisibleCart = !this.isVisibleCart;\n    }\n  },\n}\n</script>\n\n\n<style>\n.header {\n  position: sticky;\n  top: 0;\n  width: 100%;\n  height: 100px;\n  background-color: rgb(214, 214, 214);\n  display: flex;\n  border: 1px solid black;\n  justify-content: flex-end;\n  align-items: center;\n  box-shadow: 2px 2px 15px;\n}\n\n.goods-search,\n.search-button {\n  font-size: 18px;\n  height: 30px;\n  border-radius: 15px;\n  box-shadow: 2px 2px 5px;\n  \n}\n.goods-search {\n  margin-right: 20px;\n  width: 300px;\n  outline: none;\n  padding: 0 10px;\n  border: none;\n}\n\n.search-button{\n  margin-right: 20%;\n  width: 100px;\n}\n\n.cart-button {\n  flex-direction: row;\n  margin-right: 20px;\n  font-size: 28px;\n  height: 50px;\n  width: 300px;\n  border-radius: 25px;\n  box-shadow: 2px 2px 5px;\n}\n\n.main_wrapper {\n  margin: 0 auto;\n  max-width: 1920px;\n  padding: 50px;\n}\n\n\n.goods-list {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-around;\n}\n\n.goods-item {\n  width: 400px;\n  height: 600px;\n  margin: 10px;\n  border: 1px solid black;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n}\n\n.goods-item_title {\n  font-size: 36px;\n  font-weight: bold;\n  margin-bottom: 15px;\n}\n\n.goods-item_price {\n  font-size: 32px;\n}\n.button_add {\n  margin-top: 50px;\nwidth: 100px;\nfont-size: 18px;\n}\n\n/* basket */\n.basket-container {\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%, -50%);\n  width: 50%;\n  min-height: 50%;\n  background-color: antiquewhite;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding: 20px;\n}\n\n.basket-header {\n  display: flex;\n  width: 50%;\n}\n.basket-item_name, .basket-item_price {\n  font-size: 28px;\n  border: 3px solid black;\n  padding: 20px;\n  width: 50%;\n  text-align: center;\n}\n.basket-item {\n  display: flex;\n  width: 50%;\n}\n.basket-item_info_name, .basket-item_info_price {\n  font-size: 28px;\n  border: 1px solid black;\n  padding: 20px;\n  width: 50%;\n  text-align: center;\n}\n</style>\n"],"sourceRoot":""}]}